<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".register_page_general_person"
    android:background="@android:color/white">

    <ImageView
        android:id="@+id/register_general_person_logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/third_page_logo"
        tools:ignore="ImageContrastCheck"
        tools:srcCompat="@drawable/third_page_logo" />


    <ImageView
        android:id="@+id/register_general_person_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.029"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.043"
        app:srcCompat="@drawable/fourth_page_register" />

    <EditText
        android:id="@+id/fullname_register"
        android:layout_width="290dp"
        android:layout_height="40dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/plaintext_registerpage"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:hint="  Fullname"
        android:inputType="textFilter"
        android:padding="5dp"
        android:textColorHint="#241212"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.063"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/register_general_person_text"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/last_name_register"
        android:layout_width="290dp"
        android:layout_height="40dp"
        android:layout_marginStart="32dp"
        android:background="@drawable/plaintext_registerpage"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:hint="  Lastname"
        android:inputType="textFilter"
        android:padding="5dp"
        android:textColorHint="#241212"
        app:layout_constraintBottom_toBottomOf="@+id/fullname_register"
        app:layout_constraintStart_toEndOf="@+id/fullname_register"
        app:layout_constraintTop_toTopOf="@+id/fullname_register"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/phone_register"
        android:layout_width="290dp"
        android:layout_height="40dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/plaintext_registerpage"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:hint="  Phone"
        android:inputType="phone"
        android:padding="5dp"
        android:textColorHint="#241212"
        app:layout_constraintStart_toStartOf="@+id/fullname_register"
        app:layout_constraintTop_toBottomOf="@+id/fullname_register"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/general_username"
        android:layout_width="290dp"
        android:layout_height="40dp"
        android:layout_marginStart="32dp"
        android:background="@drawable/plaintext_registerpage"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:hint="  USERNAME"
        android:inputType="textPersonName"
        android:padding="5dp"
        android:textColorHint="#241212"
        app:layout_constraintBottom_toBottomOf="@+id/phone_register"
        app:layout_constraintStart_toEndOf="@+id/phone_register"
        app:layout_constraintTop_toTopOf="@+id/phone_register"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/general_password"
        android:layout_width="290dp"
        android:layout_height="40dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/plaintext_registerpage"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:hint="  PASSWORD"
        android:inputType="textShortMessage|textPersonName|textPassword|textEmailAddress"
        android:padding="5dp"

        android:textColorHint="#241212"
        app:layout_constraintStart_toStartOf="@+id/phone_register"
        app:layout_constraintTop_toBottomOf="@+id/phone_register"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/email_register"
        android:layout_width="290dp"
        android:layout_height="40dp"
        android:layout_marginStart="32dp"
        android:background="@drawable/plaintext_registerpage"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:hint="  Email"
        android:inputType="textEmailAddress"
        android:padding="5dp"
        android:textColorHint="#241212"
        app:layout_constraintBottom_toBottomOf="@+id/general_password"
        app:layout_constraintStart_toEndOf="@+id/general_password"
        app:layout_constraintTop_toTopOf="@+id/general_password"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/back_button_general_person"
        android:layout_width="150dp"
        android:layout_height="49dp"
        android:backgroundTint="#ffff"
        android:fontFamily="@font/poppins"
        android:text="ย้อนกลับ"
        android:textAllCaps="false"
        android:textColor="@color/black"
        android:textColorHighlight="#131313"
        android:textColorHint="#292626"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.048"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_register"
        app:layout_constraintVertical_bias="0.85"
        app:strokeColor="#908EF8"
        app:strokeWidth="2dp" />

    <Button
        android:id="@+id/register_general_button"
        android:layout_width="150dp"
        android:layout_height="49dp"
        android:backgroundTint="#FFFF"
        android:fontFamily="@font/poppins"
        android:text="ลงทะเบียน"
        android:textColor="#151414"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.958"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_register"
        app:layout_constraintVertical_bias="0.85"
        app:strokeColor="#908EF8"
        app:strokeWidth="2dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="402dp"
        android:layout_height="130dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/fifth_page_ellipse_1" />

</androidx.constraintlayout.widget.ConstraintLayout>